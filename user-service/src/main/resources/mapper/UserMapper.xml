<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.coderstory.springboot.userservice.mapper.UserMapper">
    <select id="findById" resultType="cn.coderstory.springboot.userservice.model.UserModel">
        select *  from user where id = #{id}
    </select>
</mapper>